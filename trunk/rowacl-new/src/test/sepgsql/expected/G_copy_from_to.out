--
-- SE-PostgreSQL testcases : COPY FROM/TO statement
--
-- selinux_user: unconfined_u
-- selinux_role: unconfined_r
-- selinux_type: sepgsql_test_t
-- selinux_range: s0:c0
-- simple copy to
COPY g1 TO stdout;
1	aaa
2	bbb
3	ccc
5	eee
6	fff
COPY g2 TO stdout;	-- to be failed
psql:./G_copy_from_to.sql:11: ERROR:  SELinux: denied { select } scontext=unconfined_u:unconfined_r:sepgsql_test_t:s0:c0 tcontext=system_u:object_r:sepgsql_secret_table_t:s0 tclass=db_column name=g2.z
-- column specified copy to
COPY g1 (security_context, a, b) TO stdout;
system_u:object_r:sepgsql_table_t:s0	1	aaa
system_u:object_r:sepgsql_table_t:s0:c0	2	bbb
system_u:object_r:sepgsql_table_t:s0	3	ccc
system_u:object_r:sepgsql_table_t:s0:c0	5	eee
system_u:object_r:sepgsql_table_t:s0	6	fff
COPY g2 (security_context, x, y) TO stdout;
unconfined_u:object_r:sepgsql_table_t:s0	1	aaa
unconfined_u:object_r:sepgsql_table_t:s0	2	bbb
unconfined_u:object_r:sepgsql_table_t:s0	3	ccc
unconfined_u:object_r:sepgsql_table_t:s0	4	ddd
COPY g2 (security_context, x, z) TO stdout;	-- to be failed
psql:./G_copy_from_to.sql:16: ERROR:  SELinux: denied { select } scontext=unconfined_u:unconfined_r:sepgsql_test_t:s0:c0 tcontext=system_u:object_r:sepgsql_secret_table_t:s0 tclass=db_column name=g2.z
-- copy to/from file
COPY g1 TO '/tmp/sepgsql_test_copy';		-- to be failed
psql:./G_copy_from_to.sql:19: ERROR:  SELinux: denied { write } scontext=unconfined_u:unconfined_r:sepgsql_test_t:s0:c0 tcontext=unconfined_u:object_r:postgresql_db_t:s0 tclass=file name=/tmp/sepgsql_test_copy
COPY g1 FROM '/tmp/sepgsql_test_copy';		-- to be failed
psql:./G_copy_from_to.sql:20: ERROR:  SELinux: denied { read } scontext=unconfined_u:unconfined_r:sepgsql_test_t:s0:c0 tcontext=unconfined_u:object_r:postgresql_db_t:s0 tclass=file name=/tmp/sepgsql_test_copy
-- copy from
COPY g1 FROM stdin;
COPY g2 FROM stdin;		-- to be failed
psql:./G_copy_from_to.sql:28: ERROR:  SELinux: denied { insert } scontext=unconfined_u:unconfined_r:sepgsql_test_t:s0:c0 tcontext=system_u:object_r:sepgsql_ro_table_t:s0 tclass=db_column name=g2.y
COPY g2 (x, y) FROM stdin;	-- to be failed
psql:./G_copy_from_to.sql:29: ERROR:  SELinux: denied { insert } scontext=unconfined_u:unconfined_r:sepgsql_test_t:s0:c0 tcontext=system_u:object_r:sepgsql_ro_table_t:s0 tclass=db_column name=g2.y
COPY g2 (x) FROM stdin;
